<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#48DC3545" />
            <size
                android:width="35dp"
                android:height="35dp" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
            <corners android:radius="@dimen/cornerRadius" />
        </shape>
    </item>
    <item android:drawable="@drawable/icon_fail" />
</layer-list>
